  "version": "4.7.2",
  "author": "Lucas Hrabovsky <hrabovsky.lucas@gmail.com> (https://imlucas.com)",
  "description": "The easiest way to test your code against MongoDB Server.",
  "scripts": {
    "check": "mongodb-js-precommit",
    "test": "mocha"
  },
  "bin": {
    "mongodb-runner": "./bin/mongodb-runner.js"
