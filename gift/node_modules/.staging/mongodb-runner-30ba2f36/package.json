    "github-release": "^0.2.0",
    "kill-mongodb": "^1.0.1",
    "lone": "^0.5.0",
    "mocha": "^5.2.0",
    "mongodb-js-precommit": "^1.0.0",
    "tmp": "0.0.33"
  }

